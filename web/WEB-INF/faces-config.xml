<?xml version='1.0' encoding='UTF-8'?>

<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
              version="2.0">
             <managed-bean>
              <managed-bean-name>databaseBean</managed-bean-name>
              <managed-bean-class>managedBeans.DatabaseBean</managed-bean-class>
              <managed-bean-scope>session</managed-bean-scope>
              </managed-bean>
              <managed-bean>
              <managed-bean-name>point</managed-bean-name>
              <managed-bean-class>managedBeans.Point</managed-bean-class>
              <managed-bean-scope>session</managed-bean-scope>
              </managed-bean>
              <managed-bean>
              <managed-bean-name>inputBean</managed-bean-name>
              <managed-bean-class>managedBeans.InputView</managed-bean-class>
              <managed-bean-scope>application</managed-bean-scope>
              </managed-bean>
              <managed-bean>
              <managed-bean-name>navigationController</managed-bean-name>
              <managed-bean-class>managedBeans.NavigationController</managed-bean-class>
              <managed-bean-scope>request</managed-bean-scope>
              <managed-property>
              <property-name>pageId</property-name>
              <value>#{param.pageId}</value>
              </managed-property>
              </managed-bean>
   				
</faces-config>